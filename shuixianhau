package hunderd;

public class hua {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=100;i<=999;i++) 
		{
			int ge = i%10;
			int shi =(i/10)%10;
			int bai =i/100;
			if(i == ge*ge*ge+shi*shi*shi+bai*bai*bai)
				System.out.println("水仙花数为:"+i);
		}
	}

}
